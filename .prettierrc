{
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxSingleQuote": false,
    "printWidth": 150,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5",
    "useTabs": false,
    "endOfLine": "auto",
    "plugins": ["@ianvs/prettier-plugin-sort-imports"],
    "importOrder": [
        "^(react/(.*)$)|^(react$)",
        "",
        "^(next/(.*)$)|^(next$)|^(next-)|^(next)",
        "",
        "<BUILTIN_MODULES>",
        "",
        "<THIRD_PARTY_MODULES>",
        "",
        "^@/components/(.*)$",
        "",
        "radix-ui$",
        "",
        "^@/lib/(.*)$",
        "",
        "^@/i18n/(.*)$",
        "",
        "<TYPES>",
        "",
        "^@/styles/(.*)$",
        "^(?!.*[.]scss$)[./].*$",
        "^(?!.*[.]css$)[./].*$",
        "",
        "^[.]"
    ],
    "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
    "importOrderTypeScriptVersion": "5.0.0",
    "importOrderCaseSensitive": false
}
